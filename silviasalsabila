def multiply_numbers(a,b):
    return a*b

# Input bilangan
bilangan_1 = [[3, 2, 1],
              [3, 4, 2],
              [5, 2, 2]]

bilangan_2 = [[2, 3, 4],
              [5, 1, 2],
              [3, 5, 1]]

# Hasil perkalian
hasil = [[multiply_numbers(bilangan_1[i][j], bilangan_2[i][j]) for j in range(3)]for i in range(3)]

# Menampilkan hasil
for row in hasil:
    print(row)
